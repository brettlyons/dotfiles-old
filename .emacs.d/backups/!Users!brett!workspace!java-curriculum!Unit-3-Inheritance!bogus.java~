class Bogus {
  private static void bogus(int someInt, float someFloat) {
    System.out.println("Some bogosity");
  }
  private static void bogus(int someInt, int otherInt) {
    System.out.println("slightly different bogosity");
  }
  private static void bogus(int anInt) {
    System.out.println("arity based bogosity");
  }
  private static void bogus(Object anObj) {
    System.out.println("object based bogosity");
  }

  public static void main(String[] args) {
    bogus(1, 0.2f);
    bogus(1, 1);
    bogus(1);
    bogus("MAKE THE OBJECT");
  }
}
